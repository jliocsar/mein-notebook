{
  "name": "@jliocsar/notiz-cli",
  "version": "0.3.3",
  "main": "notiz.ts",
  "type": "module",
  "bin": {
    "notiz": "./notiz.ts"
  },
  "scripts": {
    "start": "bun run --bun index.civet",
    "transpile": "civet -c *.civet lib/*.civet lib/db/*.civet lib/utils/*.civet && sed -i 's/.civet/.civet.tsx/g' *.tsx lib/*.tsx lib/db/*.tsx lib/utils/*.tsx",
    "postinstall": "bun transpile"
  },
  "devDependencies": {
    "@types/yargs": "latest",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "latest"
  },
  "dependencies": {
    "@danielx/civet": "latest",
    "@inquirer/prompts": "latest",
    "@libsql/client": "latest",
    "cli-markdown": "latest",
    "colorette": "latest",
    "cryptify": "latest",
    "dayjs": "latest",
    "mongodb": "latest",
    "on-exit-leak-free": "latest",
    "parse-md": "latest",
    "pupa": "latest",
    "yargs": "latest",
    "zod": "latest"
  }
}
